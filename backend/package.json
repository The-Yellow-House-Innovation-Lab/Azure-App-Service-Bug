{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "license": "UNLICENSED",
  "scripts": {
    "gen-env-vars": "npx gen-env-types .env -o src/env.d.ts -e .",
    "start": "node ./dist/src/index.js",
    "clean-ports": "chmod +x ./scripts/clean-ports.sh",
    "start-ssl-proxy": "local-ssl-proxy --source 4001 --target 4000 --key cert.key --cert cert.crt",
    "clean-dist-and-rebuild": "(rm -f -r ./dist && tsc --build ./tsconfig.json)",
    "dev": "chmod +x ./scripts/withEnv && . ./scripts/withEnv && yarn clean-ports && yarn clean-dist-and-rebuild && (yarn start-ssl-proxy & yarn watch & nodemon ./dist/src/index.js)",
    "watch": "tsc -w",
    "build": "rm -rf dist && tsc --build ./tsconfig.json",
    "gen-migration": "chmod +x ./scripts/generateMigration && . ./scripts/generateMigration",
    "dev-docker": "docker-compose -f scripts/docker/compose/dev/docker-compose.yml up -d && docker attach cilia-backend",
    "install-docker": "docker-compose -f ./scripts/docker/compose/dev/docker-compose.builder.yml run --rm install"
  },
  "keywords": [],
  "author": "The Yellow House Innovation Lab Inc.",
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "apollo-server-express": "3.9.0",
    "argon2": "0.28.5",
    "clarifai-nodejs-grpc": "8.0.0",
    "class-validator": "^0.13.1",
    "cloudinary": "^1.27.1",
    "connect-redis": "6.1.3",
    "cors": "2.8.5",
    "dataloader": "2.1.0",
    "dotenv-safe": "8.2.0",
    "express": "4.18.1",
    "express-session": "1.17.3",
    "form-data": "^4.0.0",
    "graphql": "^15.3.0",
    "graphql-type-json": "^0.3.2",
    "ioredis": "5.0.6",
    "node-video-duration": "^1.0.1",
    "nodemailer": "6.7.5",
    "pg": "8.7.3",
    "redis": "4.1.0",
    "reflect-metadata": "0.1.13",
    "source-map-support": "^0.5.19",
    "tsc": "^2.0.4",
    "type-graphql": "1.1.1",
    "typeorm": "0.2.45",
    "typeorm-text-search": "^1.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
