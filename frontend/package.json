		"@google/model-viewer": "1.12.0",
		"@next/bundle-analyzer": "^12.0.4",
		"@types/quill": "^2.0.9",
		"@types/react-date-range": "^1.4.1",
		"@types/uuid": "^8.3.4",
		"clarifai": "2.9.1",
		"cookie-cutter": "^0.2.0",
		"date-fns": "^2.27.0",
		"formik": "2.2.9",
		"framer-motion": "6.3.11",
		"get-user-media-promise": "1.1.4",
		"graphql": "^15.3.0",
		"graphql-tag": "2.12.6",
		"isomorphic-fetch": "^3.0.0",
		"isomorphic-unfetch": "3.1.0",
		"local-ssl-proxy": "1.3.0",
		"next": "12.2.0",
