{
	"name": "cilia-frontend",
	"version": "1.0.0",
	"private": true,
  "author": "The Yellow House Innovation Lab Inc.",
	"scripts": {
		"graphql:gen": "graphql-codegen --config codegen.local.yml",
		"analyze": "cross-env ANALYZE=true next build",
		"clean-ports": "chmod +x ./scripts/clean-ports.sh",
		"create-cert": "./scripts/create-cert.sh",
		"start-ssl-proxy": "local-ssl-proxy --source 3001 --target 3000 --key cert.key --cert cert.crt",
		"dev": "yarn clean-ports && yarn start-ssl-proxy & NODE_TLS_REJECT_UNAUTHORIZED=0 next dev",
		"build": "next build",
		"start": "next start -p $PORT",
		"dev-docker": "docker-compose -f scripts/docker/compose/dev/docker-compose.yml up",
		"install-docker": "docker-compose -f ./scripts/docker/compose/dev/docker-compose.builder.yml run --rm install"
	},
	"dependencies": {
		"@apollo/client": "3.6.9",
		"@babel/core": "7.18.5",
		"@chakra-ui/portal": "2.0.2",
		"@chakra-ui/react": "2.2.1",
		"@dnd-kit/core": "^6.0.3",
		"@dnd-kit/modifiers": "^6.0.0",
